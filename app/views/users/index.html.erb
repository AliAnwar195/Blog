<% if current_user.has_role? :admin%>
<h2 class="text-center" style="margin-top: 50px; color:DodgerBlue">Users</h2>
<ul class="list-group">
    <% @users.each do |user| %>
        <li class="list-group-item">
            <%= user.email %>
        </li> 
    <% end %>
</ul>
   

<%else %>
    <%= render partial:'users/error' %>
<% end %>
